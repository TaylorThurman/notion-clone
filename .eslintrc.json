{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "indent": ["error", 2],
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "never",
      "imports": "never",
      "exports": "never",
      "functions": "never"
    }],
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }]
  }
}
